{
  "name": "@dcs/ngx-components",
  "version": "1.4.0",
  "description": "Starting point for creating a npm package, including angular build support.",
  "main": "./bundles/ngx-starter.umd.js",
  "module": "./esm5/index.js",
  "es2015": "./esm15/index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "demo": "webpack-dev-server --config webpack-demo.config.js",
    "clean:build": "rimraf tmp build .rpt2_cache coverage report docs",
    "build:copy:src": "cpy 'src/**/*.*' ngx-components.ts tmp/inlined --parents",
    "build:copy:readme": "cpy 'README.md' 'build'",
    "build:copy:meta": "cpy '**/*.d.ts' '*.metadata.json' '../../build' --parents --cwd='tmp/es2015'",
    "build:package-json": "ngx-build-package-json -s package.json -d build/package.json",
    "build:inline": "ngx-inline-styles --cssGlob 'tmp/inlined/src/**/*.scss' --componentsGlob 'tmp/inlined/src/**/*.component.ts' --postcssConfig config/postcss.config.js",
    "build:aot:es2015": "ngc -p tsconfig.es2015.json",
    "build:aot:es5": "ngc -p tsconfig.es5.json",
    "build:aot": "run-s build:aot:es2015 build:aot:es5",
    "build:rollup:es2015": "rollup -c config/rollup.es2015.js",
    "build:rollup:es5": "rollup -c config/rollup.es5.js",
    "build:rollup": "run-s build:rollup:es2015 build:rollup:es5",
    "build:cleanup": "rimraf tmp",
    "build:code": "run-s build:copy:src build:inline build:aot build:copy:meta build:copy:readme build:package-json build:rollup build:cleanup",
    "build": "run-s clean:build lint test:coverage test:coverage:check build:code docs",
    "test": "jasmine-ts",
    "test:watch": "chokidar 'spec/**/*.ts' 'src/**/*.ts' --initial -c 'npm run test'",
    "test:coverage": "nyc  -e .ts -x \"*.spec.ts\" jasmine-ts \"spec/**/*.spec.ts\"",
    "test:coverage:check": "nyc check-coverage",
    "lint": "tslint --project tsconfig.json",
    "docs": "typedoc --out docs --excludeExternals --ignoreCompilerErrors --includeDeclarations --target ES6 --mode file --tsconfig tsconfig.docs.json ./src",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "commit": "git cz",
    "commitmsg": "validate-commit-msg",
    "precommit": "run-s lint",
    "prepush": "npm run build && git add docs && git commit --no-verify -m 'Auto update docs'",
    "version": "run-s build changelog && git add CHANGELOG.md",
    "postversion": "git push --no-verify && git push --no-verify --tags"
  },
  "keywords": [
    "npm",
    "rollup",
    "angular",
    "typescript"
  ],
  "author": "Marc Remolt <marc.remolt@dcs-fuerth.de>",
  "license": "ISC",
  "repository": "dcsfuerth/ngx-components",
  "devDependencies": {
    "@angular/common": "^4.4.4",
    "@angular/compiler": "^4.4.4",
    "@angular/compiler-cli": "^4.4.4",
    "@angular/core": "^4.4.4",
    "@angular/forms": "^4.4.4",
    "@angular/platform-browser": "^4.4.4",
    "@angular/platform-browser-dynamic": "^4.4.4",
    "@angularclass/hmr-loader": "^3.0.4",
    "@dcs/ngx-build-tools": "^1.5.0",
    "@types/flatpickr": "^3.0.2",
    "@types/jasmine": "^2.6.0",
    "@types/jasminewd2": "^2.0.3",
    "@types/lorem-ipsum": "^1.0.2",
    "@types/memoizee": "^0.4.1",
    "@types/node": "^8.0.31",
    "@types/numeral": "0.0.22",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.1.4",
    "bootstrap": "^4.0.0-beta",
    "chokidar-cli": "^1.2.0",
    "codelyzer": "^3.2.0",
    "commitizen": "^2.9.6",
    "conventional-changelog-cli": "^1.3.3",
    "core-js": "^2.5.1",
    "cpy-cli": "^1.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.0.0",
    "fork-ts-checker-webpack-plugin": "^0.2.8",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "jsdom": "^11.2.0",
    "loader-utils": "^1.1.0",
    "moment": "^2.18.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "numeral": "^2.0.6",
    "nyc": "^11.2.1",
    "performance-now": "^2.1.0",
    "postcss": "^6.0.12",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-absolute-module-fix": "0.0.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "^2.0.1",
    "rxjs": "^5.4.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "ts-loader": "^2.3.7",
    "ts-node": "^3.3.0",
    "tslib": "^1.7.1",
    "tslint": "^5.7.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.5.3",
    "uglify-es": "^3.1.2",
    "validate-commit-msg": "^2.14.0",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.9.1",
    "zone.js": "^0.8.18"
  },
  "peerDependencies": {
    "@angular/common": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/forms": "^4.3.0",
    "numeral": "^2.0.6",
    "rxjs": "^5.4.1"
  },
  "dependencies": {
    "flatpickr": "^3.0.7",
    "lorem-ipsum": "^1.0.4",
    "memoizee": "^0.4.11"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
